.report-edit-wrap{
  height: calc(100vh - 134rpx);
  position: relative;
}

.report-edit-swiper{
  height: calc(100% - 12rpx);
}

.swiper-wrap{
  height: 100%;
  position: relative;
}

.edit-swiper-item{
  height: 100%;
  margin: 0 6rpx;
  position: relative;
}

.edit-swiper-title{
  height: 144rpx;
  background-color: #409eff;
  border-radius: 48rpx 48rpx 48rpx 48rpx;
  position: relative;
  z-index: 2;
  text-align: center;
  line-height: 64rpx;
  color: #fff;
  font-weight: 500;
  box-shadow: 0 2rpx 5px inset #fff;
}

.report-edit-list{
  width: 100%;
  height: calc(100% - 64rpx);
  overflow-y: scroll;
  padding:12rpx 24rpx;
  margin: 0 6rpx;
  position: absolute;
  top: 64rpx;
  box-sizing: border-box;
  z-index: 3;
}
.blur-wrap::after{
  content: '';
  position: absolute;
  top:0;
  left:0;
  right:0;
  bottom: 0;
  border-radius: 48rpx;
  background-image: linear-gradient(90deg,rgba(255, 255, 255, .2),rgba(255, 2555, 255, .3),rgba(255, 255, 255, .2));
}

.edit-panel{
  margin:24rpx;
  border-radius: 24rpx;
  overflow: hidden;
}

.edit-panel-title{
  padding: 12rpx 24rpx;
  display: flex;
  color: #fff;
  background-color: #3c2efd;
  justify-content: space-between;
  z-index: 4;
}


.report-add{
  width: 64rpx;
  height: 64rpx;
  box-sizing: border-box;
  border-radius: 50%;
  position: absolute;
  background-image: url('data:image/svg+xml;base64,');
  bottom: -24rpx;
  right: 12rpx;
  background-size: 100%;
  background-color: #fff;
}

.report-add-hover{
  background-image: url('data:image/svg+xml;base64,');
  box-shadow: 0 0 5px #fff;
  background-color: #eee;
}

.report-submit{
  width: 64rpx;
  height: 64rpx;
  box-sizing: border-box;
  border-radius: 50%;
  position: absolute;
  background-image: url('data:image/svg+xml;base64,');
  bottom: 48rpx;
  right: 12rpx;
  background-size: 100%;
  background-color: #fff;
}

.report-submit-hover{
  background-image: url('data:image/svg+xml;base64,');
  box-shadow: 0 0 5px #fff;
  background-color: #eee;
}